{
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "strict": true,
    "skipLibCheck": false,
    "noImplicitOverride": true,
    // "noUnusedParameters": true,
    // "noUnusedLocals": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": true,
    "resolveJsonModule": true
    // "typeRoots": [
    //   "src/types"
    // ]
  }
}
